  </span><span class="nl">"next"</span><span class="p">:</span><span class="w"> </span><span class="s2">"https://api.organizacao.com.br/open-insurance/insurance-patrimonial/v1/insurance-patrimonial"</span><span class="p">,</span><span class="w">
  </span><span class="nl">"last"</span><span class="p">:</span><span class="w"> </span><span class="s2">"https://api.organizacao.com.br/open-insurance/insurance-patrimonial/v1/insurance-patrimonial"</span><span class="w">
