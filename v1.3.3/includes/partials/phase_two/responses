          </span><span class="nl">"code"</span><span class="p">:</span><span class="w"> </span><span class="s2">"R$"</span><span class="p">,</span><span class="w">
          </span><span class="nl">"description"</span><span class="p">:</span><span class="w"> </span><span class="s2">"BRL"</span><span class="w">
        </span><span class="p">}</span><span class="w">
      </span><span class="p">},</span><span class="w">
      </span><span class="nl">"coveragesTermStartDate"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2022-12-31"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"coveragesTermEndDate"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2022-12-31"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"coveragesTerm"</span><span class="p">:</span><span class="w"> </span><span class="mi">100</span><span class="p">,</span><span class="w">
